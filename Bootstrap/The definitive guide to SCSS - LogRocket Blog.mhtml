@charset "utf-8";

.inline-demo { width: 100%; max-width: 700px; display: none; margin-bottom: 2rem; transition: filter 0.5s ease 0s; padding: 1.5rem; border-radius: 5px; filter: drop-shadow(rgb(175, 175, 175) 5px 0px 15px); background: linear-gradient(-45deg, rgb(118, 74, 188), rgb(122, 73, 165), rgb(73, 116, 165), rgb(74, 74, 74)) 0% 0% / 400% 400%; animation: 10s ease 0s infinite normal none running gradient; }

.inline-demo:hover { filter: drop-shadow(rgb(215, 215, 215) 1px 0px 5px) !important; }

@keyframes gradient { 
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

#blog-personalized-demo-link:hover { text-decoration: none; }

.personalize-button { background: rgb(74, 188, 118); width: 35%; border-radius: 25px; text-decoration: none; color: rgb(255, 255, 255); line-height: 18px; display: block; margin: 20px auto 5px; border: 0px; padding: 20px; letter-spacing: 0.1px; box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 50px 10px; transition: all 0.25s linear 0s; }

.personalize-button:hover { color: rgb(74, 188, 118); cursor: pointer; background: rgb(255, 255, 255) !important; }

@media (max-width: 600px) {
  .personalize-button { width: 80%; }
}
