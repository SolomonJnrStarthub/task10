footer.footer a { color: rgb(118, 74, 188); }

.carousel-excerpt .fontlight, body { font-family: "PT Sans"; font-size: 15px; font-weight: 400; line-height: 1.5; color: rgb(102, 102, 102); }

.h1, .h2, .h3, .h4, .h5, .h6, h1, h2, h3, h4, h5, h6 { font-family: "PT Sans"; font-weight: 700; color: rgb(17, 17, 17); }

.carousel-excerpt .title { font-family: "PT Sans"; font-size: 30px; font-weight: 700; text-transform: none; color: rgb(255, 255, 255); }

.carousel-excerpt .fontlight { font-size: 18px; font-weight: 400; color: rgb(255, 255, 255); }

.mediumnavigation .navbar-brand { font-family: Merriweather; font-size: 26px; font-weight: 700; }

.navbar-toggleable-md .navbar-collapse { font-family: "PT Sans"; font-weight: 400; }

.article-post { font-family: Merriweather; font-weight: 400; line-height: 1.8; color: rgb(34, 34, 34); }

.mediumnavigation, .dropdown-menu, .dropdown-item { background-color: rgb(255, 255, 255); }

.mediumnavigation, .mediumnavigation a, .navbar-light .navbar-nav .nav-link { color: rgb(118, 74, 188); }

.navbar-light .navbar-brand { color: rgb(17, 17, 17); }

.navbar-light .navbar-brand:hover { color: rgb(2, 184, 117); }

.customarea .btn.follow { border-color: rgb(255, 255, 255); color: rgb(255, 255, 255); }

@media (max-width: 767px) {
  .navbar-collapse { background-color: rgb(255, 255, 255); }
}

